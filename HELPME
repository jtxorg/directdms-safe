    --email jtx@smart-moneygroup.com --agree-tos --no-eff-email \
    --config-dir /etc/letsencrypt \
    -d docuworks.avidian.com \
    --staging -vvv



docker-compose run --rm --entrypoint "" certbot \
  certonly --webroot -w /var/www/certbot \
  --email jtx@smart-moneygroup.com --agree-tos --no-eff-email \
  --config-dir /etc/letsencrypt \
  -d docuworks.avidian.com \
  --staging -vvv


docker-compose run --rm --entrypoint "" certbot \
  certbot certonly --webroot -w /var/www/certbot \
  --email jtx@smart-moneygroup.com --agree-tos --no-eff-email \
  --config-dir /etc/letsencrypt \
  -d docuworks.avidian.com \
  --staging -vvv


docker-compose run --rm certbot \
  certbot certonly --webroot -w /var/www/certbot \
  --email jtx@smart-moneygroup.com --agree-tos --no-eff-email \
  --config-dir /etc/letsencrypt \
  -d docuworks.avidian.com -vvv


docker-compose run --rm certbot \
  -c 'certbot certonly --webroot -w /var/www/certbot --email jtx@smart-moneygroup.com --agree-tos --no-eff-email --config-dir /etc/letsencrypt -d docuworks.avidian.com -vvv'


