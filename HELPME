docker-compose run --rm certbot certonly --webroot -w /var/www/certbot \
    --email jtx@smart-moneygroup.com --agree-tos --no-eff-email \
    --config-dir /etc/letsencrypt \
    -d avid.smart-moneygroup.com \
    --staging -vvv



